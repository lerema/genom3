#!/bin/sh
. ${0%${0##*/}}../test.sh

mkdir -p run.t


# --- codel parameter ------------------------------------------------------
#
testing '[$codel parameter]'

cat >run.gen <<EOF
component c {
  task t;
  ids { long a; };
  port out c::ids a;
  activity s(in long a) {
    task t;
    codel<e> c(local in a, ids in a::b, port out a::c) yield e;
  };
};
EOF
run 0  ${GENOM_BIN} interactive -b run.gen <<'EOF'
foreach p [[[[dotgen component] service] codel] parameter] {
  puts -nonewline "[$p name] "
}
puts ""
EOF
expect out '
	/^a b c $/ {next}
	{exit 2}
'

run 0 ${GENOM_BIN} interactive -b run.gen <<'EOF'
foreach p [[[[dotgen component] service] codel] parameter in] {
  puts -nonewline "[$p name] "
}
puts ""
EOF
expect out '
	/^a b $/ {next}
	{exit 2}
'
